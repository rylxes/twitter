{
  "name": "rylxes/twitter-in-laravel",
  "description": "Reformed Twitter API for Laravel",
  "keywords": [
    "twitter",
    "laravel",
    "laravel4",
    "laravel5"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "rylxes",
      "email": "rylxes@gmail.com"
    }
  ],
  "require": {
    "php": ">=5.5.0",
    "illuminate/support": "4.*|5.*",
    "themattharris/tmhoauth": "0.8.4"
  },
  "require-dev": {
    "phpunit/phpunit": "^4.8 || ^5.7|| ^6.0",
    "mockery/mockery": "~0.9.2",
    "orchestra/testbench": "^3.5"
  },
  "autoload": {
    "psr-4": {
      "Rylxes\\Twitter\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Rylxes\\Twitter\\Test\\": "tests"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Rylxes\\Twitter\\TwitterServiceProvider"
      ]
    },
    "aliases": {
      "Twitter": "Rylxes\\Twitter\\Facades\\Twitter"
    }
  },
  "minimum-stability": "stable"
}
